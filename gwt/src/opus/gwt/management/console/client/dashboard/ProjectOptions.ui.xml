<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../resources/ProjectDashboard.css" type="opus.gwt.management.console.client.resources.ProjectDashboardCss.ProjectDashboardStyle"/>
	<g:HTMLPanel styleName="{style.innerSubMainPanel}">
		<g:DockLayoutPanel unit='PCT' styleName="{style.topSubMainPanel}" >
			<g:north size='6.5'>
				<g:FlowPanel>
					<g:Label styleName="{style.pageTitle}">
						Application Settings
					</g:Label>
				</g:FlowPanel>
			</g:north>
			
			<g:south size='93'>
				<g:HTMLPanel>
					<g:Label ui:field="WarningLabel"></g:Label>
				
				<g:FormPanel ui:field="UserSettingsForm">
					<g:FlexTable ui:field="formContainer">
					
					</g:FlexTable>
				</g:FormPanel>
				<g:Button ui:field="SaveButton" text="Save"></g:Button>
				<g:Button ui:field="ActivateButton" text="Save and Activate"></g:Button>
				</g:HTMLPanel>
			</g:south>
		</g:DockLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 