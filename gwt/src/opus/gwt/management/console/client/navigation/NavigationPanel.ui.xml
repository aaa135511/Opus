<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../resources/NavigationPanel.css" />
		
	<g:SimplePanel>
	 	<g:HTMLPanel styleName="{style.navigationContainer}">
	 		<g:HTMLPanel styleName="{style.logo}">
	 			Opus Management Console
	 		</g:HTMLPanel>
	 		<g:Button ui:field="authenticationButton" styleName="{style.authenticationButton}">Logout</g:Button>	 			
	 		<g:Button ui:field="dashboardsButton" styleName="{style.button}">My Projects</g:Button>
			<g:Button ui:field="deployNewButton" styleName="{style.button}">Deploy New Project</g:Button>
			<g:Button ui:field="loggedInUserButton" styleName="{style.button} {style.username}"></g:Button>
	 	</g:HTMLPanel>
	</g:SimplePanel>
</ui:UiBinder> 