<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../resources/NavigationPanel.css" />
		
	<g:SimplePanel>
	 	<g:HTMLPanel styleName="{style.navigationContainer}">
	 		<g:HTMLPanel styleName="{style.logo}">
	 			Opus Management Console
	 		</g:HTMLPanel>
	 		<g:Button ui:field="authenticationButton" addStyleNames="{style.authenticationButton} (style.buttonT}">Logout</g:Button>	 			
	 		<g:Button ui:field="dashboardsButton" addStyleNames="{style.topDashboardButton} (style.buttonT}">My Projects</g:Button>
			<g:Button ui:field="deployNewButton" addStyleNames="(style.buttonT} {style.topDeployNewButton}">Deploy New Project</g:Button>
			<g:Button ui:field="loggedInUserButton" addStyleNames=" (style.buttonT} {style.loggedInUserButton}">edhelms</g:Button>
	 	</g:HTMLPanel>
	</g:SimplePanel>
</ui:UiBinder> 