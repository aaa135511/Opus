<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../resources/NavigationPanel.css" type="opus.gwt.management.console.client.resources.NavigationPanelCss.NavigationPanelStyle"/>
		
	<g:SimplePanel>
	 	<g:HTMLPanel styleName="{style.navigationContainer}">
	 		<h1>Opus Management Console</h1>
	 		<g:HTMLPanel ui:field="buttonHTMLPanel" styleName="{style.menu}">	
		 		<g:FormPanel ui:field='logoutForm' styleName="{style.logoutPanel}">
			 			<g:Button ui:field="logoutButton" styleName="{style.button} {style.logoutButton}">Logout</g:Button>
			 	</g:FormPanel>
			 	<g:Button ui:field="projectsButton" styleName="{style.button}">My Projects</g:Button>
				<g:Button ui:field="deployNewButton" styleName="{style.button}">Deploy New Project</g:Button>
		 		<g:Button ui:field="loggedInUserButton" styleName="{style.button} {style.username}"></g:Button>
			</g:HTMLPanel>
	 	</g:HTMLPanel>
	</g:SimplePanel>
	
</ui:UiBinder> 