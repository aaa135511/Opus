<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../resources/TooltipPanel.css" type="opus.gwt.management.console.client.resources.TooltipPanelCss.TooltipPanelStyle"/>
	<g:HTMLPanel styleName="{style.tooltip}">
		<g:HTMLPanel styleName="{style.tooltip_left}" ui:field="arrow" />
		<g:HTMLPanel styleName="{style.tooltip_right}" ui:field="content">
			<g:HTML ui:field="text" />
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 