<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="resources/PanelManager.css" field="managerStyle" type="opus.gwt.management.console.client.resources.PanelManagerCss.PanelManagerStyle"/>
	<ui:style src="resources/ProjectManager.css" field="style" />
	<g:ScrollPanel ui:field='iconScrollPanel' styleName="{style.managerDeckPanel}">
		<g:HTMLPanel>
			<g:HTMLPanel styleName="{style.header}">
				<g:HTMLPanel styleName="{style.insetType}">
					<h1>Dashboard</h1>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:FlowPanel ui:field="projectIconsFlowPanel" />
		</g:HTMLPanel>
	</g:ScrollPanel>
</ui:UiBinder> 